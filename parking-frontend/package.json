{
    "name": "parking-frontend",
    "version": "1.0.0",
    "description": "Frontend del Sistema de Gestión de Parqueaderos Distribuidos",
    "author": "ESPE Universidad",
    "license": "MIT",
    "keywords": [
        "parqueadero",
        "parking",
        "gestión",
        "sistema distribuido",
        "frontend"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/PamelaChipe/Distibuidas_Parqueadero"
    },
    "scripts": {
        "serve": "python -m http.server 8000",
        "serve-node": "npx http-server",
        "dev": "npm run serve",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "e2e": "cypress run",
        "e2e:open": "cypress open",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "test:all": "npm run test:coverage && npm run lint && npm run e2e"
    },
    "devDependencies": {
        "cypress": "^13.6.0",
        "eslint": "^8.53.0",
        "eslint-config-recommended": "^4.1.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.2.0"
    }
}
